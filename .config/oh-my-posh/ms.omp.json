{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "auto_upgrade": true,
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "name": "Show shell",
          "background": "#0077c2",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "template": " \ue626 {{ .Name }} ",
          "type": "shell"
        },
        {
          "name": "Show current time",
          "background": "#2e9599",
          "foreground": "#111111",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "name": "Show username",
          "background": "#c386f1",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "name": "Show working directory",
          "background": "#ff479c",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "template": " \uea83  {{ .Path }} ",
          "type": "path"
        },
        {
          "name": "Show git info",
          "background": "#fffb38",
          "foreground": "#193549",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "name": "Show node info",
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "properties": {
            "fetch_version": true
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "name": "Show go info",
          "background": "#8ED1F7",
          "foreground": "#111111",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "properties": {
            "fetch_version": true
          },
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "name": "Show go info",
          "background": "#4063D8",
          "foreground": "#111111",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "properties": {
            "fetch_version": true
          },
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        {
          "name": "Show python info",
          "background": "#FFDE57",
          "foreground": "#111111",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "name": "Show ruby info",
          "background": "#AE1401",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "name": "Show azure info",
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "template": " \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "azfunc"
        },
        {
          "name": "Show aws info",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "properties": {
            "display_default": false
          },
          "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "type": "aws"
        },
        {
          "name": "Show is user root info",
          "background": "#ffff66",
          "foreground": "#111111",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "template": " \uf0ad ",
          "type": "root"
        },
        {
          "name": "Show execution time",
          "background": "#83769c",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "properties": {
            "always_enabled": true
          },
          "template": " \ueba2 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "name": "Show execution status",
          "background": "#00897b",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\ue0b8",
          "background_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "template": " \ue23a ",
          "type": "status"
        },
        {
          "name": "Add new line",
          "type": "text",
          "style": "plain",
          "foreground": "#E06C75",
          "template": "\n‚ùØ"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true
}
